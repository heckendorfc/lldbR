#PKG_CFLAGS = @LLDB_CFLAGS@
#PKG_LIBS = @LLDB_LIBS@
OBEXTRA = @OBEXTRA@

OBJECTS = raw.o

all: $(SHLIB)


$(SHLIB): $(OBJECTS)
$(OBJECTS): $(OBEXTRA)


mkclldb:
	( mkdir -p build/libs; cd lldb; gmake BUILD_DIR=../build/libs)

instclldb: mkclldb
	mkdir -p ../inst/libs
	cp build/libs/* ../inst/libs/

clean:
	rm -rf ./build
